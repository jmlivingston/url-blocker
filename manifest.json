{
  "name": "URL Blocker",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "*://news.google.com/*",
        "*://*.linkedin.com/*",
        "*://*.twitter.com/*",
        "*://*.facebook.com/*"
      ],
      "js": ["blocker.js"],
      "run_at": "document_start"
    }
  ]
}
